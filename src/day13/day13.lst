__freemem:
;        db      $dd,01,00,00
        ld      bc,(__heapptr)
        ld      (hl),c
        inc     hl
        ld      (hl),b
        inc     hl
        ld      (hl),e
        inc     hl
        ld      (hl),d
        dec     hl
        dec     hl
        dec     hl
        ld      (__heapptr),hl
        ret

